{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "sql",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_statement"
      }
    },
    "_statement": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "select_statement"
        },
        {
          "type": "SYMBOL",
          "name": "table_expression"
        }
      ]
    },
    "_list_separator": {
      "type": "STRING",
      "value": ","
    },
    "_quoted_string": {
      "type": "PATTERN",
      "value": "\"[^\"]*\""
    },
    "_reference": {
      "type": "PATTERN",
      "value": "[a-zA-Z0-9_]+"
    },
    "_name_or_string": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_reference"
        },
        {
          "type": "SYMBOL",
          "name": "_quoted_string"
        }
      ]
    },
    "_alias": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "as"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "FIELD",
          "name": "alias",
          "content": {
            "type": "SYMBOL",
            "name": "alias"
          }
        }
      ]
    },
    "alias": {
      "type": "SYMBOL",
      "name": "_name_or_string"
    },
    "select_statement": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "select",
          "flags": "i"
        },
        {
          "type": "SYMBOL",
          "name": "select_list"
        }
      ]
    },
    "select_list": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "select_list_item"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "_list_separator"
              },
              {
                "type": "SYMBOL",
                "name": "select_list_item"
              }
            ]
          }
        }
      ]
    },
    "column_name": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_column_table_reference"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "_name_or_string"
        }
      ]
    },
    "select_list_item": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "reference",
          "content": {
            "type": "SYMBOL",
            "name": "column_name"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_alias"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "_column_table_reference": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "column_table_reference",
          "content": {
            "type": "SYMBOL",
            "name": "_name_or_string"
          }
        },
        {
          "type": "STRING",
          "value": "."
        }
      ]
    },
    "table_expression": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "from",
          "flags": "i"
        },
        {
          "type": "SYMBOL",
          "name": "table_name"
        }
      ]
    },
    "table_name": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_name_or_string"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_alias"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
